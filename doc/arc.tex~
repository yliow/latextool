\section{arc}

\begin{python}
from latextool_basic import execute
execute(r"""
from latextool_basic import Plot, grid, arc
p = Plot()
s = arc(x=0, y=0, r=1, angle0=0, angle1=45, linewidth=0.1, color='red')
p.add(s)
p.add(grid, x0=-1, y0=-1, x1=1, y1=1)
print p
""", print_source=True)
\end{python}


